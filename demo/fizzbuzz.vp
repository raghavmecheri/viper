bool func isDivisible(int x, int div) => (x % div == 0);

for(int i = 1; i <= 100; i+=1) {
    if(isDivisible(i, 15)) {
        print("fizzbuzz");
    }
    else if(isDivisible(i, 3)) {
        print("fizz");
    } else if(isDivisible(i, 5)) {
        print("buzz");
    } else {
        print(i);
    }
}
