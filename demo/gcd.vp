int func conditional(int x, int y) {
    if(x == 0 && y == 0) {
        return -1;
    }
    return (x == 0 ? y : x);
}

int func gcd(int a, int b) {
    int check = conditional(a, b);
    if(check == -1) {
        int diff = a > b ? (a-b) : (b-a);
        return a > b ? gcd(diff, b) : gcd(a, diff);
    }
    return check;
}

print(gcd(100, 10));
